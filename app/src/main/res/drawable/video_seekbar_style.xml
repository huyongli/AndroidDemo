<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:startColor="#ff666666"
                android:centerColor="#ff666666"
                android:centerY="0.75"
                android:endColor="#ff666666"
                android:angle="270"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#ffb4b4b4"
                    android:centerColor="#ffb4b4b4"
                    android:centerY="0.75"
                    android:endColor="#ffb4b4b4"
                    android:angle="270"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#ffe72c00"
                    android:centerColor="#ffe72c00"
                    android:centerY="0.75"
                    android:endColor="#ffe72c00"
                    android:angle="270"/>
            </shape>
        </clip>
    </item>
</layer-list>